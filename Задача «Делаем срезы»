s=input()
n=(len(s))
print (s[2])
print (s[-2])
print (s[:5])
print (s[:-2])
print (s[0:n:2])
print (s[1:n:2])
print (s[::-1])
print (s[::-2])
print (len(s))


s = input()
print(s[2])
print(s[-2])
print(s[:5])
print(s[:-2])
print(s[::2])
print(s[1::2])
print(s[::-1])
print(s[::-2])
print(len(s))


a=input()
print(a[2])
print(a[-2])
print(a[0:5])
b=len(a)
print(a[0:b-2])
print(a[0:b:2])
print(a[1:b:2])
print(a[::-1])
print(a[::-2])
print(b)
