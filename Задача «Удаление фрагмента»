s=input()
a=s.find('h')
b=s.rfind('h')
s1=s[:a]+s[b+1:]
print(s1)


s = input()
s = s[:s.find('h')] + s[s.rfind('h') + 1:]
print(s)


n = input()
x = n.find('h')
y = n.rfind('h')+1
print (n[:x] + n[y:])


s = input()
s1 = s[:s.find('h')]
s2 = s[s.rfind('h') + 1:]
print(s1 + s2)


s = input()
a = s.find('h')
b = s.rfind('h')
print(s.replace(s[a:b+1],''))


